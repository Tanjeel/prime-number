n = int(input("Enter a number to check: "))
def isPrime(n): 
  
    
    if n <= 1: 
        return False
  
     
    for i in range(2, n): 
        if n % i == 0: 
            return False; 
  
    return True
  

print(n, "is a Prime number") if isPrime(n) else print(n, "is not a Prime number ")
